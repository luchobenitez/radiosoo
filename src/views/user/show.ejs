<div class="container">

  <div class="resume">
      <header class="page-header">
      <h1 class="page-title"><%= user.name %></h1>
      <small> <i class="fa fa-clock-o"></i> Last Updated on: <time><%=user.modifiedAt %></time></small>
    </header>
    <div class="row">
      <div class="col-xs-12 col-sm-12 col-md-offset-1 col-md-10 col-lg-offset-2 col-lg-8">
        <div class="panel panel-default">
          <div class="panel-heading resume-heading">
            <div class="row">
              <div class="col-lg-12">
                <div class="col-xs-12 col-sm-4">
                  <figure>
                    <img class="img-circle img-responsive" alt="" src="http://placehold.it/300x300">
                  </figure>
                </div>
                <div class="col-xs-12 col-sm-8">
                  <ul class="list-group">
                    <li class="list-group-item"><%= user.name %></li>
                    <li class="list-group-item"><i class="fa fa-phone"></i> 000-000-0000 </li>
                    <li class="list-group-item"><i class="fa fa-envelope"></i> <%= user.email %></li>
                    <li class="list-group-item"> Registrado el: <%=user.registeredAt %></li>
                    <li class="list-group-item"> Modificado el: <%=user.modifiedAt %></li>
                    <li class="list-group-item"> Ultimo Login: <%=user.lastLogin %></li>
                    <li class="list-group-item"> Visto el : <%=user.lastSeen %></li>
                    <li class="list-group-item"> Ultimo Post: <%=user.lastPost %></li>
                    <li class="list-group-item"> Ultimo mail: <%=user.lastEmail %></li>
                    <li class="list-group-item"> Estado : <%=user.status %></li>
                    <li class="list-group-item"> Nombre: <%= user.name %></li>
                    <li class="list-group-item"> Fecha de Nacimiento: <%= user.dateOfBirth %></li>
                    <li class="list-group-item"> Nick: <%= user.nick %></li>
                    <li class="list-group-item"> Username: <%= user.username %></li>
                    <li class="list-group-item"> Password: <%= user.password %></li>
                  </ul>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
